{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "prisma:seed": "ts-node-dev --transpile-only prisma/seed.ts",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "dev": "export NODE_ENV=development && concurrently --kill-others \"npm run watch\" \"nodemon dist/server.js\"",
    "start": "NODE_ENV=production node dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "nodemon": "^3.1.10",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "prisma": "^6.14.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
