{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "cleanup": "pkill -f 'node.*workers' || true",
    "db:seed": "ts-node-dev prisma/seed.ts",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "start:workers": "node dist/workers/index.js",
    "purge:queues": "ts-node src/scripts/purgeQueues.ts",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "dev": "export NODE_ENV=development && concurrently --kill-others \"npm run watch\" \"nodemon dist/server.js\"",
    "start": "NODE_ENV=production node dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "amqplib": "^0.10.8",
    "bcrypt": "^6.0.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "isolation-forest": "^0.0.9",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.5",
    "nodemon": "^3.1.10",
    "openai": "^5.13.1",
    "pdf-parse": "^1.1.1",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tesseract.js": "^6.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^6.0.0",
    "@types/csv-parse": "^1.1.12",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/node-fetch": "^2.6.13",
    "@types/nodemailer": "^7.0.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/pdfjs-dist": "^2.10.377",
    "@types/redis": "^4.0.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/tesseract.js": "^0.0.2",
    "@types/xlsx": "^0.0.35",
    "prisma": "^6.14.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
